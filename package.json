{
  "name": "maidbot-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "tw": "npx tailwindcss -i ./styles/globals.css -o ./dist/output.css --watch",
    "dev": "concurrently \"sudo service mysql start\" \"npm run tw\" \"npm run dev:next\" ",
    "dev:next": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "eslint": "eslint --fix",
    "studio": "pnpx prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "prettier-format": "prettier --config .prettierrc '{Pages,Components,Utils}/**/*{.ts,.tsx,.js,.jsx}' --write",
    "pre-commit-checks": "concurrently \"npm run prettier-format\" \"npm run lint\" ",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-format && npm run lint",
      "pre-push": ""
    }
  },
  "dependencies": {
    "axios": "^1.2.4",
    "dotenv": "^16.0.3",
    "next": "13.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "simple-crypto-js": "^3.0.1"
  },
  "devDependencies": {
    "@google-cloud/text-to-speech": "^4.1.0",
    "@jest/globals": "^29.4.0",
    "@prisma/client": "^4.9.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.5",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.4.0",
    "jest-environment-jsdom": "^29.4.0",
    "jest-extended": "^3.2.3",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.21",
    "prettier": "2.8.3",
    "prisma": "^4.9.0",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
