{
  "private": true,
  "scripts": {
    "tw": "npx tailwindcss -i ./styles/global.css -o ./dist/output.css --watch",
    "dev": "concurrently \"sudo service mysql start\" \"npm run tw\" \"npm run dev:next\"",
    "dev:next": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "eslint": "eslint --fix",
    "studio": "pnpx prisma studio",
    "prettier-format": "prettier --config .prettierrc '{Pages,Components,Utils}/**/*{.ts,.tsx,.js,.jsx}' --write",
    "pre-commit-checks": "concurrently \"npm run prettier-format\" \"npm run lint\" ",
    "prepare": "husky install",
    "pretty-quick": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-format && npm run lint",
      "pre-push": ""
    }
  },
  "dependencies": {
    "@testing-library/dom": "^8.20.0",
    "@typescript-eslint/parser": "^5.0.0",
    "axios": "^1.2.4",
    "dotenv": "^16.0.3",
    "next": "latest",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "simple-crypto-js": "^3.0.1"
  },
  "devDependencies": {
    "@google-cloud/text-to-speech": "^4.1.0",
    "@jest/globals": "^29.4.0",
    "@prisma/client": "^4.9.0",
    "@testing-library/dom": "7.21.4",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/user-event": "14.2.0",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "@types/react": "18.0.9",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.5",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-testing-library": "^5.10.0",
    "husky": "^8.0.3",
    "jest": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "jest-extended": "^3.2.3",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.21",
    "prettier": "2.8.3",
    "pretty-quick": "^3.1.3",
    "prisma": "^4.9.0",
    "tailwindcss": "^3.2.4",
    "typescript": "4.6.4"
  }
}
