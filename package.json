{
  "name": "maidbot-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "tw": "npx tailwindcss -i ./styles/globals.css -o ./dist/output.css --watch",
    "dev": "concurrently \"sudo service mysql start\" \"npm run tw\" \"npm run dev:next\" ",
    "dev:next": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "studio": "pnpx prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "prettier-format": "prettier --config .prettierrc '{Pages,Components,Utils}/**/*{.ts,.tsx,.js,.jsx}' --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-format && npm lint",
      "pre-push": ""
    }
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "next": "12.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "simple-crypto-js": "^3.0.1"
  },
  "devDependencies": {
    "@google-cloud/text-to-speech": "^4.0.0",
    "@prisma/client": "^4.9.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^18.0.4",
    "@types/react": "^18.0.15",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.6.0",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "postcss": "^8.4.14",
    "prettier": "2.8.3",
    "prisma": "^4.9.0",
    "tailwindcss": "^3.2.4",
    "typescript": ">=4.7.4"
  }
}
